# Copy this file to .env.local and fill in your values

# Google Apps Script URL (from your deployment)
NEXT_PUBLIC_GOOGLE_SCRIPT_URL=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec

# OpenAI API Key (optional, for AI extraction)
OPENAI_API_KEY=sk-proj-XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://YOUR_PROJECT_ID.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key-here

# Google Sheets Template (for creating new user sheets)
GOOGLE_SHEET_TEMPLATE_ID=your-template-sheet-id

# Market Data APIs
# US Census Bureau API - Get your key at: https://api.census.gov/data/key_signup.html
NEXT_PUBLIC_CENSUS_API_KEY=

# Federal Reserve Economic Data (FRED) API - Get your key at: https://fred.stlouisfed.org/docs/api/api_key.html
NEXT_PUBLIC_FRED_API_KEY=

# Bureau of Labor Statistics (BLS) API - Register at: https://data.bls.gov/registrationEngine/
NEXT_PUBLIC_BLS_API_KEY=

# Optional: Business marketplace APIs (if available)
# NEXT_PUBLIC_BIZBUYSELL_API_KEY=
# NEXT_PUBLIC_LOOPNET_API_KEY=

# Optional: Additional data sources
# NEXT_PUBLIC_GOOGLE_PLACES_API_KEY=
# NEXT_PUBLIC_YELP_API_KEY=

# Apify API Token - Get your token at: https://console.apify.com/account/integrations
APIFY_API_TOKEN=


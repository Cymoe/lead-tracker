# Copy this file to .env.local and fill in your values

# Google Apps Script URL (from your deployment)
NEXT_PUBLIC_GOOGLE_SCRIPT_URL=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec

# OpenAI API Key (optional, for AI extraction)
OPENAI_API_KEY=your-openai-api-key-here

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=***REMOVED***
NEXT_PUBLIC_SUPABASE_ANON_KEY=***REMOVED***

# Google Sheets Template (for creating new user sheets)
GOOGLE_SHEET_TEMPLATE_ID=your-template-sheet-id

